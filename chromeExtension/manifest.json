{
  "name": "seeULater_beta",
  "version": "1.0.8",
  "description": "have yourself",
  "homepage_url": "https://www.seeulater.kr/",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "browser_action.html"
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "MacCtrl+Shift+Y"
      },
      "description": "Send a 'toggle-feature' event to the extension"
    },
    "store_page": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Ctrl+Shift+S"
      },
      "description": "Store this tab's url"
    }
  },
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "alligator_128.png",
    "32": "alligator_128.png",
    "48": "alligator_128.png",
    "128": "alligator_128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "manifest_version": 3
}